{
  "workflows": [
    {
      "name": "main",
      "variables": {
        "test_value": "3"
      },
      "nodes": {
        "start": {
          "opcode": "workflow_start",
          "next": "if_check",
          "inputs": {}
        },
        "if_check": {
          "opcode": "control_if_else",
          "inputs": {
            "CONDITION": { "node": "is_greater" },
            "THEN": { "branch": "print_yes" },
            "ELSE": { "branch": "print_no" }
          }
        },
        "is_greater": {
          "opcode": "operator_equals",
          "inputs": {
            "OPERAND1": { "variable": "test_value" },
            "OPERAND2": { "literal": "5" }
          },
          "isReporter": true
        },
        "print_yes": {
          "opcode": "io_print",
          "inputs": {
            "STRING": { "literal": "Value is 5!\n" }
          }
        },
        "print_no": {
          "opcode": "io_print",
          "inputs": {
            "STRING": { "literal": "Value is not 5!\n" }
          }
        }
      }
    }
  ]
}
