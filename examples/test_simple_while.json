{
  "workflows": [
    {
      "name": "main",
      "variables": {
        "counter": "1"
      },
      "nodes": {
        "start": {
          "opcode": "workflow_start",
          "next": "while_loop",
          "inputs": {}
        },
        "while_loop": {
          "opcode": "control_while",
          "inputs": {
            "CONDITION": { "node": "check_counter" },
            "SUBSTACK": { "branch": "print_and_increment" }
          }
        },
        "check_counter": {
          "opcode": "operator_less_than",
          "inputs": {
            "OPERAND1": { "variable": "counter" },
            "OPERAND2": { "literal": "4" }
          },
          "isReporter": true
        },
        "print_and_increment": {
          "opcode": "io_print",
          "next": "print_newline",
          "inputs": {
            "STRING": { "node": "format_counter" }
          }
        },
        "format_counter": {
          "opcode": "data_get_variable",
          "inputs": {
            "VARIABLE": { "literal": "counter" }
          },
          "isReporter": true
        },
        "print_newline": {
          "opcode": "io_print",
          "next": "increment",
          "inputs": {
            "STRING": { "literal": "\n" }
          }
        },
        "increment": {
          "opcode": "data_set_variable_to",
          "inputs": {
            "VARIABLE": { "literal": "counter" },
            "VALUE": { "node": "add_one" }
          }
        },
        "add_one": {
          "opcode": "operator_add",
          "inputs": {
            "OPERAND1": { "variable": "counter" },
            "OPERAND2": { "literal": "1" }
          },
          "isReporter": true
        }
      }
    }
  ]
