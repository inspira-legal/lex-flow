{
  "workflows": [
    {
      "name": "main",
      "variables": {
        "1" : ["model",""],
        "2" : ["agent",""]
      },
      "nodes": {
        "start": {
          "opcode": "event_start",
          "next": "save_model",
          "inputs": {}
        },
        "create_model": {
          "opcode": "ai_create_model",
          "inputs": {
            "model_name": [1, "gemini-2.5-flash"]
          }
        },
        "save_model": {
          "opcode": "data_set_variable_to",
          "inputs": {
            "variable": [1, "1"],
            "value": [2, "create_model"]
          },
          "next": "save_agent"
        },
        "save_agent": {
          "opcode": "data_set_variable_to",
          "inputs": {
            "var": [1, "2"],
            "value1": [2, "create_agent"]
          },
          "next": "print_output"
        },
        "create_agent": {
          "opcode": "ai_create_agent",
          "inputs": {
            "model": [3, "1"],
            "system_prompt": [1, "Answer correctly and helpfully."],
            "agent_name" : [1,"Guriz√£o"]
          }
        },
        "call_agent": {
          "opcode": "ai_call_agent",
          "inputs": {
            "agent": [3, "2"],
            "user_prompt": [1, "Write a python function that prints the Fibonacci sequence."]
          }
        },
        "print_output": {
          "opcode": "io_print",
          "inputs": {
            "string": [2, "call_agent"]
          }
        }
      }
    }
  ]
}
