{
  "workflows": [
    {
      "name": "main",
      "interface": {
        "inputs": [],
        "outputs": []
      },
      "variables": {
        "1": ["temp1", ""],
        "2": ["temp2", ""]
      },
      "nodes": {
        "start": {
          "opcode": "workflow_start",
          "next": "push_args",
          "inputs": {}
        },
        "push_args": {
          "opcode": "data_set_variable_to",
          "next": "push_second_arg",
          "inputs": {
            "VARIABLE": [1, "1"],
            "VALUE": [1, "7"]
          }
        },
        "push_second_arg": {
          "opcode": "data_set_variable_to",
          "next": "call_workflow",
          "inputs": {
            "VARIABLE": [1, "2"],
            "VALUE": [1, "3"]
          }
        },
        "call_workflow": {
          "opcode": "workflow_call",
          "inputs": {
            "WORKFLOW": [1, "add_numbers"],
            "ARG1": [3, "1"],
            "ARG2": [3, "2"]
          }
        }
      }
    },
    {
      "name": "add_numbers",
      "interface": {
        "inputs": ["x", "y"],
        "outputs": ["sum"]
      },
      "variables": {
        "1": ["x", 0],
        "2": ["y", 0]
      },
      "nodes": {
        "start": {
          "opcode": "workflow_start",
          "next": "calculate",
          "inputs": {}
        },
        "calculate": {
          "opcode": "operator_add",
          "next": "print_result",
          "inputs": {
            "OPERAND1": [3, "1"],
            "OPERAND2": [3, "2"]
          }
        },
        "print_result": {
          "opcode": "io_print",
          "next": "return_result",
          "inputs": {
            "STRING": [1, "Function result!\n"]
          }
        },
        "return_result": {
          "opcode": "workflow_return",
          "inputs": {
            "VALUE": [2, "calculate"]
          }
        }
      }
    }
  ]
