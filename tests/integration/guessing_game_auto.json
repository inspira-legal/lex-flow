{
  "workflows": [
    {
      "name": "main",
      "variables": {
        "1": ["secret_number", "5"],
        "2": ["test_guess", "5"]
      },
      "nodes": {
        "start": {
          "opcode": "workflow_start",
          "next": "welcome_message",
          "inputs": {}
        },
        "welcome_message": {
          "opcode": "io_print",
          "next": "test_correct_guess",
          "inputs": {
            "STRING": [1, "Testing guessing game logic...\n"]
          }
        },
        "test_correct_guess": {
          "opcode": "control_if_else",
          "inputs": {
            "CONDITION": [2, "is_correct"],
            "BRANCH1": [4, "correct_message"],
            "BRANCH2": [4, "wrong_message"]
          }
        },
        "is_correct": {
          "opcode": "operator_equals",
          "inputs": {
            "OPERAND1": [3, "2"],
            "OPERAND2": [3, "1"]
          },
          "isReporter": true
        },
        "correct_message": {
          "opcode": "io_print",
          "next": "test_comparison",
          "inputs": {
            "STRING": [1, "Equality test PASSED!\n"]
          }
        },
        "wrong_message": {
          "opcode": "io_print",
          "next": "test_comparison",
          "inputs": {
            "STRING": [1, "Equality test FAILED!\n"]
          }
        },
        "test_comparison": {
          "opcode": "io_print",
          "next": "test_greater",
          "inputs": {
            "STRING": [1, "Testing operators...\n"]
          }
        },
        "test_greater": {
          "opcode": "control_if_else",
          "inputs": {
            "CONDITION": [2, "is_greater"],
            "BRANCH1": [4, "greater_pass"],
            "BRANCH2": [4, "greater_fail"]
          }
        },
        "is_greater": {
          "opcode": "operator_greater_than",
          "inputs": {
            "OPERAND1": [1, "7"],
            "OPERAND2": [1, "5"]
          },
          "isReporter": true
        },
        "greater_pass": {
          "opcode": "io_print",
          "inputs": {
            "STRING": [1, "Greater than test PASSED!\n"]
          }
        },
        "greater_fail": {
          "opcode": "io_print",
          "inputs": {
            "STRING": [1, "Greater than test FAILED!\n"]
          }
        }
      }
    }
  ]
}
