{
  "workflows": [
    {
      "name": "main",
      "variables": {
        "1": ["secret_number", "0"],
        "2": ["user_guess", "0"],
        "3": ["game_running", "true"]
      },
      "nodes": {
        "start": {
          "opcode": "event_start",
          "next": "generate_number",
          "inputs": {}
        },
        "generate_number": {
          "opcode": "data_set_variable_to",
          "next": "welcome_message",
          "inputs": {
            "VARIABLE": [1, "1"],
            "VALUE": [2, "random_1_to_10"]
          }
        },
        "random_1_to_10": {
          "opcode": "math_random",
          "inputs": {
            "MIN": [1, "1"],
            "MAX": [1, "10"]
          },
          "isReporter": true
        },
        "welcome_message": {
          "opcode": "io_print",
          "next": "game_loop",
          "inputs": {
            "STRING": [1, "Welcome to the Guessing Game!\nI'm thinking of a number between 1 and 10.\n"]
          }
        },
        "game_loop": {
          "opcode": "control_while",
          "inputs": {
            "CONDITION": [2, "check_game_running"],
            "SUBSTACK": [4, "get_user_input"]
          }
        },
        "check_game_running": {
          "opcode": "operator_equals",
          "inputs": {
            "OPERAND1": [3, "3"],
            "OPERAND2": [1, "true"]
          },
          "isReporter": true
        },
        "get_user_input": {
          "opcode": "io_print",
          "next": "read_input",
          "inputs": {
            "STRING": [1, "Enter your guess: "]
          }
        },
        "read_input": {
          "opcode": "data_set_variable_to",
          "next": "check_guess",
          "inputs": {
            "VARIABLE": [1, "2"],
            "VALUE": [2, "input_number"]
          }
        },
        "input_number": {
          "opcode": "io_input",
          "inputs": {},
          "isReporter": true
        },
        "check_guess": {
          "opcode": "control_if_else",
          "next": null,
          "inputs": {
            "CONDITION": [2, "is_correct"],
            "BRANCH1": [4, "correct_guess"],
            "BRANCH2": [4, "wrong_guess"]
          }
        },
        "is_correct": {
          "opcode": "operator_equals",
          "inputs": {
            "OPERAND1": [3, "2"],
            "OPERAND2": [3, "1"]
          },
          "isReporter": true
        },
        "correct_guess": {
          "opcode": "io_print",
          "next": "end_game",
          "inputs": {
            "STRING": [1, "Congratulations! You guessed it!\n"]
          }
        },
        "end_game": {
          "opcode": "data_set_variable_to",
          "inputs": {
            "VARIABLE": [1, "3"],
            "VALUE": [1, "false"]
          }
        },
        "wrong_guess": {
          "opcode": "control_if_else",
          "inputs": {
            "CONDITION": [2, "is_too_high"],
            "BRANCH1": [4, "too_high_message"],
            "BRANCH2": [4, "too_low_message"]
          }
        },
        "is_too_high": {
          "opcode": "operator_greater_than",
          "inputs": {
            "OPERAND1": [3, "2"],
            "OPERAND2": [3, "1"]
          },
          "isReporter": true
        },
        "too_high_message": {
          "opcode": "io_print",
          "inputs": {
            "STRING": [1, "Too high! Try again.\n"]
          }
        },
        "too_low_message": {
          "opcode": "io_print",
          "inputs": {
            "STRING": [1, "Too low! Try again.\n"]
          }
        }
      }
    }
  ]
}